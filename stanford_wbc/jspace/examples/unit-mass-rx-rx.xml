<?xml version="1.0" ?>
<!-- Two masses of 1kg each, attached in series at a distance of 1m
     from the origin and each other along the Y-axis, each rotating
     around the X-axis going through its parent -->
<dynworld>
  <baseNode>
    <gravity>0, 0, -9.81</gravity>
    <pos>0, 0, 0</pos>
    <rot>1, 0, 0, 0</rot>
    <jointNode>
      <ID>0</ID>
      <type>R</type>
      <axis>X</axis>
      <mass>1</mass>
      <inertia>0, 0, 0</inertia>
      <com>0, 1, 0</com>
      <pos>0, 0, 0</pos>
      <rot>0, 0, 1, 0</rot>
      <jointNode>
        <ID>1</ID>
        <type>R</type>
        <axis>X</axis>
        <mass>1</mass>
        <inertia>0, 0, 0</inertia>
        <com>0, 1, 0</com>
        <pos>0, 1, 0</pos>
        <rot>0, 0, 1, 0</rot>
      </jointNode>
    </jointNode>
  </baseNode>
</dynworld>
